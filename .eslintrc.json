{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "class-methods-use-this": 0,
    "react/prop-types": 0,
    "id-length": 0,
    "indent": ["error", 2],
    "no-console": 0,
    "no-unused-vars": [
      1,
      {
        "vars": "local",
        "args": "all"
      }
    ],
    "strict": 0
  },
  "globals": {
    "document": false
  }
}
